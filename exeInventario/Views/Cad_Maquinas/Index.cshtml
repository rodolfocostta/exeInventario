@model IEnumerable<exeInventario.Models.Cad_Maquinas>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Cadastrar</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Modelo_Maq)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Peças_Maq)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Modelo_Maq)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Peças_Maq)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID_Maq">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID_Maq">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID_Maq">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
